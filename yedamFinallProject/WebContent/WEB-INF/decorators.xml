<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/adminviews">
    <decorator name="adminTheme" page="navTop.jsp">            
        <pattern>*.ad</pattern>
    </decorator>
    
    <!-- 레이아웃 구분해서 적용  일반사용자 / 관리자 -->
    <excludes>
    <!-- parameter 값이 넘어갈 경우 주소 뒤에 ?param=1&.. 형식으로 넘어가기 때문에
    	 주소 뒤 * 을 붙여야 같이 excludes됨 -->
    	<pattern>/AdminAjaxSelngAnalysisDayOk.ad*</pattern>        
    	<pattern>/AdminAjaxSelngAnalysisWeekOk.ad*</pattern>     
    	<pattern>/AdminAjaxSelngAnalysisMonthOk.ad*</pattern>
    	<pattern>/AdminAjaxGoodsAnalysQyOk.ad*</pattern>
    	<pattern>/AdminAjaxGoodsAnalysAmountOk.ad*</pattern>
    	<pattern>/AdminAjaxSellerAnalysQyOk.ad*</pattern>
    	<pattern>/AdminAjaxSellerAnalysAmountOk.ad*</pattern>
    </excludes>
     
</decorators>